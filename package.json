{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "public-ip": "^3.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-geosuggest": "^2.9.0",
